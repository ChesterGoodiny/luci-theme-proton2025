{
    "luci-theme-proton2025": {
        "description": "Proton2025 theme - temperature sensor, setting access, and services monitoring",
        "read": {
            "ubus": {
                "luci.proton-temp": [
                    "getSensors",
                    "resetPeaks"
                ],
                "luci.proton-settings": [
                    "getSettings"
                ]
            },
            "uci": [
                "proton2025"
            ],
            "file": {
                "/etc/init.d/*": [
                    "exec"
                ],
                "/bin/pidof": [
                    "exec"
                ]
            }
        },
        "write": {
            "ubus": {
                "luci.proton-settings": [
                    "setSetting",
                    "setSettings",
                    "resetSettings"
                ]
            },
            "uci": [
                "proton2025"
            ]
        }
    }
}